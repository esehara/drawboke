{
  "name": "drawboke",
  "private": true,
  "version": "0.0.1",
  "description": "皆で絵とお題を繋げてボケる",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esehara/drawboke.git"
  },
  "bugs": {
    "url": "https://github.com/esehara/drawboke/issues"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src/**/*.ts* --fix",
    "prettier": "prettier",
    "fmt": "run-s prettier lint",
    "git": "run-s lint; git add -A; git commit",
    "start": "run-p count dev:** watch:**",
    "build": "webpack --mode=development --progress",
    "build:log": "webpack --json --progress --profile > webpack-build-log.json",
    "dev:js": "webpack watch --mode=development --progress",
    "dev:serve": "firebase emulators:start",
    "watch:fmt": "npm-watch fmt",
    "count": "printf '\\e[36m\n  This project has\\e[35m %s lines\\e[36m of source code!\n\n\\e[m' $(find ./src -name '*.html' -o -name '*.css' -o -name '*.js*' -o -name '*.ts*' -o -name '*.json' | xargs wc -l | tail -1 | awk '{print $1}')"
  },
  "watch": {
    "fmt": "{src,test}/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@types/fabric": "^4.2.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.25.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-tsc": "^2.0.0",
    "firebase-tools": "^9.10.2",
    "jest": "^26.6.3",
    "material-js": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.9.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.1",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@types/react-color": "^3.0.4",
    "boxicons": "^2.0.7",
    "feather-icons": "^4.28.0",
    "firebase": "^8.4.2",
    "framer-motion": "^4.1.11",
    "install": "^0.13.0",
    "konva": "^7.2.5",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-konva": "*",
    "react-router-dom": "^5.2.0"
  }
}
